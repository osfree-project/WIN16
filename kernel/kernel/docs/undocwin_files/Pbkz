		},

		ended: function(){
			if(typeof(console)=='object' && typeof(console.log)=='function') console.log('ads ended');

			var cn = {'ru': 1,'by':1,'kz':1,'uz':1,'am':1, 'md':1, 'az':1, 'tj':1, 'kg':1, 'tm':1, 'ua':1, 'ge':1};
